{
  "name": "aurorastaking",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:local": "hardhat deploy",
    "deploy:auroraTestnet": "hardhat deploy --network auroraTestnet",
    "deploy:auroraMainnet": "hardhat deploy --network auroraMainnet",
    "deploy:goerli": "hardhat deploy --network goerli",
    "clean": "rm -rf artifacts/ cache/ typechain/ coverage/ coverage.json",
    "coverage": "npm run clean; env COVERAGE=true hardhat coverage",
    "lint": "npm run lint:js && npm run lint:sol",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix",
    "lint:js": "npx eslint **/*.ts",
    "lint:js:fix": "npx eslint **/*.ts --fix",
    "lint:sol": "solhint 'contracts/**/*.sol' && prettier -c 'contracts/**/*.sol'",
    "lint:sol:fix": "prettier --write \"contracts/**/*.sol\"",
    "generate:doc": "npx hardhat docgen"
  },
  "author": "",
  "license": "ISC",
